  <h1><%=@survey.title%></h1>
  <h2 class="italics"><%= @survey.description %></h2>
  <%= erb :"question/_create", layout: false, locals: {survey: @survey} %>
  <ol id='questions'>
    <% @survey.questions.each_with_index do |question| %>
      <%= erb :"#{question.partial}", layout: false, locals: {question: question} %>
    <% end %>
  </ul>
